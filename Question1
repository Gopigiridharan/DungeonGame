public class DungeonGame1 {

	public static void main(String[] args) {

		int i1 = 5, i2 = 1, j1 = 1, j2 = 4, count = 1;

		while (i1 != i2 && j1 != j2) {
			if (i1 < i2) {
				i1++;
				count++;
			} else {
				i1--;
				count++;
			}
			if (j1 < j2) {
				j1++;
				count++;
			} else {
				j1--;
				count++;
			}
		}
		System.out.println(count);
	}
}
